{
    "family": "console-sample-app",
    "volumes": [
        {
            "name": "my-vol",
            "host": {}
        }
    ],
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "name": "simple-app",
            "image": "410628466457.dkr.ecr.us-east-1.amazonaws.com/ecs-node-simple-app",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ]
}
